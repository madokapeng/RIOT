config BOARD
    default "esp8266-wifi-shield" if BOARD_ESP8266_WIFI_SHIELD

config BOARD_ESP8266_WIFI_SHIELD
    bool
    default y
    select BOARD_COMMON_ESP8266
    select CPU_MODEL_ESP8266_ESP_12X

source "$(RIOTBOARD)/common/esp8266/Kconfig"
